    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.1",
    "cssnano": "^5.0.14",
    "daisyui": "^1.21.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-check-updates": "^12.1.0",
